<main>
  <h1>Ironhack Cinema</h1>
  <p>Click on the movie to check the showtimes!</p>
  <section class="movies-list">
    {{#each movies as |movie|}}
    <article class="movie">
      <img src={{this.image}} alt={{this.title}}>
      <h2>{{this.title}}</h2>
      <a href="/movies/{{this._id}}">See more</a>
    </article>
    {{/each}}
  </section>
</main>